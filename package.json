{
  "name": "express-ts-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "node ./util/build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "cross-env ENV_FILE=production node -r ./env -r module-alias/register ./dist/start.js",
    "start:dev": "nodemon --config \"./util/nodemon.json\"",
    "test": "nodemon --config \"./util/nodemon.test.json\""
  },
  "_moduleAliases": {
    "@middlewares": "dist/middleware",
    "@shared": "dist/shared",
    "@server": "dist/Server",
    "@ts": "dist/ts",
    "@validators": "dist/validators"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "helmet": "^3.21.3",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/shortid": "0.0.29",
    "dotenv": "^8.2.0",
    "fs-extra": "^8.1.0",
    "nodemon": "^1.19.4",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "winston": "^3.2.1"
